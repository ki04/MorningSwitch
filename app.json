{
  "expo": {
    "name": "MorningSwitch",
    "slug": "MorningSwitch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "morningswitch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "jsc",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kijeong.MorningSwitch",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-firebase-core",
        {
          "config": {
            "apiKey": "AIzaSyCypOTcA3XGumte3OUzpN2ZflmJmXtHlI8",
            "authDomain": "morning-switch-app-60505.firebaseapp.com",
            "projectId": "morning-switch-app-60505",
            "storageBucket": "morning-switch-app-60505.appspot.com",
            "messagingSenderId": "23756271152",
            "appId": "1:23756271152:web:942a5bcecaf6775556c7f5",
            "measurementId": "G-M3CPBXR7B9"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2091ee7a-5098-40e0-a1b8-59fef54b4d6b"
      }
    }
  }
}
